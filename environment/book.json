{
	"title" : "新书" ,
	"author" : "李姜丰",
	"description" : "记录Gitbook的配置和一些插件的使用",
	"language" : "zh-hans" ,
	


	"plugins": [
    	"-search",
    	 "-lunr",
    	 "search-pro",
    	"back-to-top-button",
    	"chapter-fold",
    	"expandable-chapters",
    	"copy-code-button",
    	"advanced-emoji",
    	"splitter",
    	"pageview-count",
    	"popup",
		"anchor-navigation-ex",
		"tbfed-pagefooter",
		"prism",
        "-highlight",
		"hide-element"
	],

	"pluginsConfig": {
		"anchor-navigation-ex": {
			"showLevel": true,
			"showGoTop":false,
			 "float": { 
				"floatIcon": "fa fa-anchor", 
				"showLevelIcon": false,  
				"level1Icon": "fa fa-hand-o-right", 
				"level2Icon": "fa fa-hand-o-right",
				"level3Icon": "fa fa-hand-o-right"
			}
		},
		"tbfed-pagefooter": {
            "copyright":"",
            "modify_label": "该文件最后修改时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
		"prism": {
            "css": [
                "prism-themes/themes/prism-darcula.css"
            ]
        },
		"hide-element": {
            "elements": [".gitbook-link"]
        }
    }

}
